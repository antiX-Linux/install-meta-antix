FLL_DESCRIPTION="GB English localisation of latest Firefox from Mozilla"


FLL_PRE_PROCESSING=(
	"#!/bin/bash"
	"echo \"---- in preprocessing ----\""
	"echo \"Adding Mozilla repository to /etc/apt/sources.list.d/various.list\""
	"sed -i -r '/mozilla/ s/^#+//' /etc/apt/sources.list.d/various.list"
        "apt-get update"
        "echo \"---- preprocessing done----\""
)

FLL_PACKAGE_DEPMODS=(
)

FLL_PACKAGES=(
      firefox-l10n-en-gb  
)
